Algoritmo "atvlista8"
//  
//  


Var
 metros ,km,hm,dam,dm,cm,mm:real


Inicio
escreva ("digite a distancia em metros:")
leia (metros)
 km <-metros/1000
 hm <-metros/100
 dam <-metros/10
 dm <-metros*10
 cm <-metros*100
 mm <-metros*1000
escreval ("a distancia de:" ,metros ,"metros corresponde a:",km ,"km" ,hm ,"hm",dam,"dam",dm,"dm",cm, "cm",mm, "mm")



Fimalgoritmo