algoritmo "Exercicio 42"

var

v,cont:inteiro

inicio

      cont<-0

      escreva("Quer contar ate quanto?")

      leia(v)

      enquanto (cont<=v) faca

          escreval(cont)

          cont<-cont+1

     fimenquanto

     escreval("terminei de contar")







fimalgoritmo