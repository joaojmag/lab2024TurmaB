algoritmo "DobroDeUmNume"
var
N,D,T:Real
inicio
      escreval("Me fale um numero(REAL OU INTEIRO) para saber o dobro e a Terça a parte dele: ")
      leia(N)
      D<-N*2
      T<-N/3
      escreval("O dobro de ", N , " é: " ,D)
      escreval("A terça a parte de ", N , " é: ", T )
fimalgoritmo