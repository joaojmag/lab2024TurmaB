algoritmo "tintaParede"
var
L,H,A,T:real
inicio
      escreval("Qual a largura da parede? ")
      leia(L)
      escreval("Qual a altura desta parede?")
      leia(H)
       A<-L*H
       T<-A/2
       Escreval("A area a ser pintada é de: ", A)
       escreval("E a quantidade de tinta é:", T , "L de tinta ")
fimalgoritmo