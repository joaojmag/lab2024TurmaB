algoritmo "EquaçãoSegundoGrau"
var
A,B,C,Del:real
inicio
      escreva("Informe a letra 'A' da Equação:")
      leia(A)
      escreva("Informe a letra 'B' da Equação:")
      leia(B)
      escreva("Informe a letra 'C' da Equação:")
      leia(C)
      del<- (b^2) - (4*A*C)
      escreva("Delta: ", del)
      
fimalgoritmo