algoritmo "Exercicio 18"
var
   ano,nas,idade:inteiro
inicio
   escreva("Em que ano estamos?(YYYY): ")
   leia(ano)
   escreva("Em que ano você nasceu(YYYY):")
   leia(nas)
   idade<-ano-nas
   se (idade>=18) entao
      escreva("Voce ja pode votar amigo!!")
   senao
      se (Idade<18)
         escreva("Ainda não pode votar.")
      fimse
   fimse

fimalgoritmo