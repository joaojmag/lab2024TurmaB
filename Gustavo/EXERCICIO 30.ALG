algoritmo "EXERCICIO 30"

var

R,D:real

cont,conversao:inteiro

inicio
      escreval("Quer conveter quantas vezes?")
      leia(conversao)
      para cont<-1 ate conversao faca

      escreval("Qual o valor em real?R$:")

      leia(R)

      D<-R/5.5

      escreval("O valor convertido e UR$:",D:5:2)

      fimpara

fimalgoritmo