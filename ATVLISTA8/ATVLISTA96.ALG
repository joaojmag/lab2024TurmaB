Algoritmo "atvlista96"

Var
   n1, n2, soma:inteiro
   media:real
Funcao m(X,Y:real):real
Inicio
   retorne m <-  (X + Y)/ 2

Fimfuncao
Inicio
   escreva("Nota 1:")
   leia(n1)
   escreva("Nota 2:")
   leia(n2)
   soma <- n1 + n2
   media <-soma/2
   escreva("Media:",media)

Fimalgoritmo