Algoritmo "atvlista92"

Var
   n:inteiro
Procedimento parouimpar(V:Inteiro)
inicio
   se (V%2 = 0)entao
      escreval("Numero:","{",V,"}", "  PAR")
   senao
      escreval("Numero: ","{", V,"}", "  IMPAR")
   fimse
fimprocedimento
Inicio
   escreva("Número:")
   leia(n)
   parouimpar(n)





Fimalgoritmo