algoritmo "semnome"
var
   contador, numero: inteiro

inicio
      escreval ("Digite o número que você deseja ver a tabuada: ")
      leia (numero)
      para contador <- 1 ate 10 faca
            escreva (numero, " x ", contador, " = " )
            escreval (numero * contador)
      fimpara


   
fimalgoritmo