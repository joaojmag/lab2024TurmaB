Algoritmo "atvlista55."
//  
//  

Var
contagem,n,sorteio:inteiro


Inicio

escreva ("digite um numero de 1 a 10:")
 leia (n)
sorteio <- randi(9)+1
   enquanto (contagem<=4)faca
 se(n=sorteio)entao
  escreva("voce acertou :)")
 senao
    se (n<>sorteio)entao
     escreva ("voce errou :(")



     fimse
    fimse
    fimenquanto



Fimalgoritmo