algoritmo "5Lista"
var
 Aluno:Caractere
 Nota1, Nota2:Real
 
inicio
 Escreva("Pesquise seu nome: ")
 Leia(Aluno)
 Nota1 <- 8,5
 Nota2 <- 4,5
 Escreval(" ")
 Escreval("Encontrado!Bem vindo " ,Aluno, "!")
 Escreval(Aluno)
 Escreval("Portugues", Nota1)
 Escreval("Matematica", Nota2)
 Escreval("Clique aqui para mais->")
fimalgoritmo