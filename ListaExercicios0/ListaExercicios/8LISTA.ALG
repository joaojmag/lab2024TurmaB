algoritmo "8Lista"
var
 Numero,km, cm, hm, dam, dm, mm: real
inicio
Escreva("Digite um medida em metros: ")
Leia(Numero )
dam<- Numero/10
hm<- Numero/20
km<- Numero/30
dm<- Numero*10
cm<-Numero*20
mm<- Numero*30
Escreval(Numero, " m")
Escreval(dam, " dam")
Escreval(hm ," hm")
Escreval(km ," km")
Escreval(dm ," dm")
Escreval(cm ," cm")
Escreval(mm ," mm")
fimalgoritmo