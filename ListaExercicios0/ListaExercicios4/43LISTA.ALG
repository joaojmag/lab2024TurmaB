algoritmo "43Lista"
var
   cont : inteiro
inicio
escreval("A contagem vai começar")
   para cont de 35 ate 1 passo -1 faca
      se (cont mod 4 = 0) entao
         escreva ("[", cont, "]")
         senao entao
         escreva(cont)
      fimse
   fimpara
   //
   escreval
   escreval("Acabou!!")
fimalgoritmo