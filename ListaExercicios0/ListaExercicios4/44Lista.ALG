algoritmo "44Lista"
var
contador, n1, n2, n3:inteiro
inicio
escreval("Primeiro valor")
leia(n1)
escreval("Ultimo valor")
leia(n2)
escreval("Incremento")
leia(n3)

contador <- n1
Enquanto (contador <= n2) faca
escreva (contador)
contador <- contador + n3
      fimenquanto
      escreval (" Acabou!")







fimalgoritmo