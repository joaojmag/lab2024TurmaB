Algoritmo "atvlista70"

Var
   contagem,n1,n2,n3:inteiro


Inicio
      n1<- 0
      escreva (n1)
      n2 <-1
      escreva (n2)

      para contagem de 3 ate 10 faca
      n3 <- n1+ n2
      escreva (n3)
      n1 <- n2
      n2 <- n3
      contagem <- contagem +1
   fimpara



Fimalgoritmo