algoritmo "ConversorDeRealParaDolar"
var
   r, d: real
inicio
      escreval (" Quer converter seu real? ")
      escreva (" Então vamos lá! Quantos R$ você quer converter? ")
      leia (r)
      d <- r/5.15
      escreval ("R$",r, " convertidos para dolar são aproximadamente US$",d)

fimalgoritmo
