algoritmo "Exercicio 90"

var

   v1,v2,s:inteiro

procedimento soma(a,b:inteiro)

inicio
   s<-a+b
fimprocedimento

inicio
     escreval("Qual o primeiro valor?")
     leia(v1)
     escreval("Qual o segundo valor?")
     leia(v2)
     soma(v1,v2)
     escreval("A soma do",v1," e ",v2, " é igual a ",s)
fimalgoritmo