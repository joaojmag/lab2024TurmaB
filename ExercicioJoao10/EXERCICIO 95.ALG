algoritmo "Exercicio 95"
var
   v1,v2,s:inteiro
funcao soma(var a,b:inteiro):inteiro
inicio
   retorne a+b
fimfuncao



inicio

     escreval("Qual o primeiro valor?")
     leia(v1)
     escreval("Qual o segundo valor?")
     leia(v2)
     s<-soma(v1,v2)
     escreval("A soma do",v1," e ",v2, " é igual a ",s)

fimalgoritmo